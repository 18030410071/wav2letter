def make_unittest(base):
    lua_unittest(
        name = base,
        srcs = [ '{}.lua'.format(base) ],
        deps = [ "@/torch/fb/torchnet:torchnet-sequential" ],
    )

make_unittest('test_bucketbatchdataset')
make_unittest('test_bucketsorteddataset')
make_unittest('test_flatindexeddataset')
make_unittest('test_sequencebatchdataset')
make_unittest('test_targetnextdataset')
make_unittest('test_truncateddatasetiterator')
